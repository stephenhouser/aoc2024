Checks: 'clang-diagnostic-*,clang-analyzer-*,cert-*,readability-identifier-naming'
WarningsAsErrors: '*'
HeaderFileExtensions: ['', 'h', 'hpp', 'hh', 'hxx']
ImplementationFileExtensions: ['c', 'cpp', 'cxx']
HeaderFilterRegex: '.*'
FormatStyle: none
User: houser
CheckOptions:
  - { key: readability-identifier-naming.GlobalFunctionCase, value: lower_case}
SystemHeaders:   false
